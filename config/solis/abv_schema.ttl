abv:aangemaakt_op a rdf:Property;
  rdfs:label "aangemaakt_op";
  rdfs:comment "datum waarop record werd aangemaakt";
  rdfs:domain abv:EntiteitMetadata;
  rdfs:range xsd:date .

abv:aangepast_op a rdf:Property;
  rdfs:label "aangepast_op";
  rdfs:comment "datum waarop record is aangepast";
  rdfs:domain abv:EntiteitMetadata;
  rdfs:range xsd:date .

abv:aantekening_archivaris a rdf:Property;
  rdfs:label "aantekening_archivaris";
  rdfs:comment "Dit veld zal alle aantekeningen bundelen die voordien in de velden “aantekening”, “werknotities” en “aantekening archivaris” stonden.";
  rdfs:domain abv:Archief;
  rdfs:range xsd:string .

abv:adres a rdf:Property;
  rdfs:label "adres";
  rdfs:comment "Adres van bewaarplaats.";
  rdfs:domain abv:Beheerder;
  rdfs:range abv:Adres .

abv:auteur a rdf:Property;
  rdfs:label "auteur";
  rdfs:comment "auteur van de publicatie";
  rdfs:domain abv:Publicatie;
  rdfs:range abv:Naam .

abv:beheerder a rdf:Property;
  rdfs:label "beheerder";
  rdfs:comment """persoon of organisatie die het archief of de collectie vandaag beheerd.
link naar wat voorheen de ‘bewaarplaats’ was. 
Nu expliciet gemaakt als een Agent, die verbonden is aan een bepaalde Plaats. """;
  rdfs:domain abv:Archief;
  rdfs:range abv:Beheerder .

abv:bereikbaarheid a rdf:Property;
  rdfs:label "bereikbaarheid";
  rdfs:comment "Indien list: Rolstoeltoegankelijk, Aangepast sanitair, Parkeergelegenheid";
  rdfs:domain abv:Beheerder;
  rdfs:range abv:Bereikbaarheid .

abv:bibliografie_agent a rdf:Property;
  rdfs:label "bibliografie_agent";
  rdfs:comment "Bronnen over de agent.";
  rdfs:domain abv:Agent;
  rdfs:range xsd:string .

abv:bibliografie_archief a rdf:Property;
  rdfs:label "bibliografie_archief";
  rdfs:comment "bibliografie werken gebaseerd op of gerelateerd aan het archief/de collectie";
  rdfs:domain abv:Archief;
  rdfs:range xsd:string .

abv:bijdragers a rdf:Property;
  rdfs:label "bijdragers";
  rdfs:comment "De namen van personen en organisaties die een bijdrage leverden aan deze waardering";
  rdfs:domain abv:Waardering;
  rdfs:range xsd:string .

abv:bron_waardering a rdf:Property;
  rdfs:label "bron_waardering";
  rdfs:comment "Verwijzing naar een publicatie die als bron werd gebruikt voor deze waardering. ";
  rdfs:domain abv:Waardering;
  rdfs:range abv:Publicatie .

abv:bronnen_agent a rdf:Property;
  rdfs:label "bronnen_agent";
  rdfs:comment "Bronnen gebruikt om agentbeschrijving op te stellen.";
  rdfs:domain abv:Agent;
  rdfs:range xsd:string .

abv:bronnen_archief a rdf:Property;
  rdfs:label "bronnen_archief";
  rdfs:comment "bibliografie werken gebruikt voor het opstellen van de beschrijving";
  rdfs:domain abv:Archief;
  rdfs:range xsd:string .

abv:definitie a rdf:Property;
  rdfs:label "definitie";
  rdfs:comment "Definitie van het Concept";
  rdfs:domain abv:Concept;
  rdfs:range rdf:langString .

abv:email a rdf:Property;
  rdfs:label "email";
  rdfs:comment "Emailadres van bewaarplaats.";
  rdfs:domain abv:Beheerder;
  rdfs:range xsd:string .

abv:functie_beroep_activiteit a rdf:Property;
  rdfs:label "functie_beroep_activiteit";
  rdfs:comment "Functie, beroep of activiteiten waarin agent ";
  rdfs:domain abv:Samensteller;
  rdfs:range abv:Functie .

abv:gebouw a rdf:Property;
  rdfs:label "gebouw";
  rdfs:comment "Verwijzing naar de bewaarplaatsen die verbonden zijn aan de Agent";
  rdfs:domain abv:Beheerder;
  rdfs:range xsd:string .

abv:geschiedenis_agent a rdf:Property;
  rdfs:label "geschiedenis_agent";
  rdfs:comment "Biografische geschiedenis van de agent.";
  rdfs:domain abv:Agent;
  rdfs:range rdf:langString .

abv:geschiedenis_archief a rdf:Property;
  rdfs:label "geschiedenis_archief";
  rdfs:comment "Geschiedenis van het archief.";
  rdfs:domain abv:Archief;
  rdfs:range rdf:langString .

abv:identificatie a rdf:Property;
  rdfs:label "identificatie";
  rdfs:comment """Herhaalbaar om verschillende IDs op te kunnen nemen.
Eigen persistente URI wordt door het systeem toegekend en is dus ‘verplicht’.
Andere IDs enkel toevoegen als ze al buiten ABV bestaan.""";
  rdfs:domain abv:Agent;
  rdfs:range abv:Identificatienummer .

abv:inhoud_en_bereik a rdf:Property;
  rdfs:label "inhoud_en_bereik";
  rdfs:comment "Inhoud van archief.";
  rdfs:domain abv:Archief;
  rdfs:range rdf:langString .

abv:label a rdf:Property;
  rdfs:label "label";
  rdfs:comment "Naam voor het Concept";
  rdfs:domain abv:Concept;
  rdfs:range rdf:langString .

abv:naam a rdf:Property;
  rdfs:label "naam";
  rdfs:comment "Herhaalbaar om naamsvarianten te documenteren.";
  rdfs:domain abv:Agent;
  rdfs:range abv:Naam .

abv:omvang a rdf:Property;
  rdfs:label "omvang";
  rdfs:comment "vrije tekst beschrijving van de dimensies en materiaalsoorten waaruit het archiefbestand bestaat";
  rdfs:domain abv:Archief;
  rdfs:range abv:Omvang .

abv:onderwerp a rdf:Property;
  rdfs:label "onderwerp";
  rdfs:comment "Onderwerp trefwoorden die geassocieerd kunnen worden met archief.";
  rdfs:domain abv:Associatie;
  rdfs:range abv:Onderwerp .

abv:opmerking a rdf:Property;
  rdfs:label "opmerking";
  rdfs:comment "Vrij opmerkingen veld.";
  rdfs:domain abv:Agent;
  rdfs:range xsd:string .

abv:opmerkingen a rdf:Property;
  rdfs:label "opmerkingen";
  rdfs:comment "Vrij notitieveld bij deze waardering";
  rdfs:domain abv:Waardering;
  rdfs:range xsd:string .

abv:periode a rdf:Property;
  rdfs:label "periode";
  rdfs:comment "Tijdsperiode waarmee archief gelinkt kan worden.";
  rdfs:domain abv:Associatie;
  rdfs:range abv:Periode .

abv:plaats a rdf:Property;
  rdfs:label "plaats";
  rdfs:comment "Plaats geassocieerd met het archief.";
  rdfs:domain abv:Associatie;
  rdfs:range abv:Plaats .

abv:plaats_van_uitgave a rdf:Property;
  rdfs:label "plaats_van_uitgave";
  rdfs:comment "naam van de plaats van uitgave";
  rdfs:domain abv:Publicatie;
  rdfs:range xsd:string .

abv:plaatsnaam a rdf:Property;
  rdfs:label "plaatsnaam";
  rdfs:comment "";
  rdfs:domain abv:Plaats;
  rdfs:range abv:Naam .

abv:raadplegingsvoorwaarde a rdf:Property;
  rdfs:label "raadplegingsvoorwaarde";
  rdfs:comment "vrije tekst omschrijving van de concrete voorwaarden die verbonden zijn aan de toegang tot het archief/collectie";
  rdfs:domain abv:Archief;
  rdfs:range abv:Raadplegingsvoorwaarde .

abv:samensteller a rdf:Property;
  rdfs:label "samensteller";
  rdfs:comment "nieuwe overkoepelende term voor zowel archiefvormers, verzamelaars als documentalisten. herhaalbaar om verschillende samenstellers en hun verschillende rollen te kunnen documenteren";
  rdfs:domain abv:Archief;
  rdfs:range abv:Samensteller .

abv:selectie a rdf:Property;
  rdfs:label "selectie";
  rdfs:comment "Verklaring over de erfgoedwaarde van het archiefbestand en de vergelijking met andere archiefbestanden.";
  rdfs:domain abv:Archief;
  rdfs:range abv:Waardering .

abv:taal_text a rdf:Property;
  rdfs:label "taal_text";
  rdfs:comment "oude taal veld";
  rdfs:domain abv:Archief;
  rdfs:range xsd:string .

abv:telefoon a rdf:Property;
  rdfs:label "telefoon";
  rdfs:comment "Telefoonnummer van bewaarplaats.";
  rdfs:domain abv:Beheerder;
  rdfs:range xsd:string .

abv:uitgever a rdf:Property;
  rdfs:label "uitgever";
  rdfs:comment "naam van de uitgever van de publicatie";
  rdfs:domain abv:Publicatie;
  rdfs:range xsd:string .

abv:url a rdf:Property;
  rdfs:label "url";
  rdfs:comment "URL die verwijst naar een digitale versie van de publicatie";
  rdfs:domain abv:Publicatie;
  rdfs:range xsd:anyURI .

abv:vergelijking a rdf:Property;
  rdfs:label "vergelijking";
  rdfs:comment "Omschrijving hoe het archiefbestand zich voor een specifiek aspect verhoudt tot andere archiefbestanden.";
  rdfs:domain abv:Waardering;
  rdfs:range abv:Vergelijking .

abv:verwerving a rdf:Property;
  rdfs:label "verwerving";
  rdfs:comment "Uitleg hoe archief verworven werd door bewaarinstelling.";
  rdfs:domain abv:Archief;
  rdfs:range rdf:langString .

abv:verwijderd_op a rdf:Property;
  rdfs:label "verwijderd_op";
  rdfs:comment "datum waarop record is verwijderd";
  rdfs:domain abv:EntiteitMetadata;
  rdfs:range xsd:date .

abv:website a rdf:Property;
  rdfs:label "website";
  rdfs:comment "Website bewaarinstelling.";
  rdfs:domain abv:Beheerder;
  rdfs:range xsd:string .

